{
  "name": "@joneff/postcss-explode-selectors",
  "description": "A postcss plugin to explode (duplicate) selectors",
  "version": "0.0.0-development",
  "author": "Ivan Zhekov",
  "license": "MIT",
  "keywords": [
    "css",
    "postcss",
    "postcss-plugin",
    "selectors",
    "explode",
    "explode-selectors",
    "selector",
    "extend",
    "extend-selector"
  ],
  "main": "index.js",
  "repository": "joneff/postcss-explode-selectors",
  "bugs": {
    "url": "https://github.com/joneff/postcss-explode-selectors/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "eslint": "eslint \"**/*.js\"",
    "test": "mocha",
    "mocha-dots": "mocha --reporter dot",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "postcss": "^8.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/core": "^11.0.0",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "eslint": "^7.31.0",
    "husky": "^4.3.8",
    "mocha": "^9.0.3",
    "postcss": "^8.0.0",
    "semantic-release": "^17.4.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint",
      "pre-push": "npm run eslint && npm run mocha-dots",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
